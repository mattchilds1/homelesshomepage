{
    "name": "HomelessHomepage",
    "version": "0.0.1",
    "manifest_version": 2,
    "description" : "Homeless homepage replaces your current new tab page, selling advertising space to help charities helping the homeless.",
    "icons": { "16": "icons/16x16.png", "48": "icons/48x48.png", "128": "icons/128x128.png" },
    "omnibox": { "keyword" : "homeless" },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "chrome_url_overrides" : {
        "newtab": "newtab/newtab.html"
    }
}